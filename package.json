{
	"name": "mf-sales-web",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"dev:uat": "cp .env.uat .env && vite dev",
		"build": "vite build",
		"build:uat": "cp .env.uat .env && vite build",
		"build:cug": "cp .env.cug .env && vite build",
		"build:prod": "cp .env.prod .env && vite build",
		"preview": "vite preview",
		"test": "playwright test",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"test:unit": "vitest",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write .",
		"prepare": "husky install",
		"storybook": "storybook dev -p 6006",
		"build-storybook": "storybook build"
	},
	"devDependencies": {
		"@playwright/test": "^1.28.1",
		"@storybook/addon-essentials": "^7.0.0-beta.53",
		"@storybook/addon-interactions": "^7.0.0-beta.53",
		"@storybook/addon-links": "^7.0.0-beta.53",
		"@storybook/blocks": "^7.0.0-alpha.8",
		"@storybook/svelte": "^7.0.0-beta.53",
		"@storybook/sveltekit": "^7.0.0-beta.53",
		"@storybook/testing-library": "^0.0.14-next.1",
		"@sveltejs/adapter-auto": "^1.0.0",
		"@sveltejs/kit": "1.8.3",
		"@typescript-eslint/eslint-plugin": "^5.45.0",
		"@typescript-eslint/parser": "^5.45.0",
		"autoprefixer": "^10.4.13",
		"cookie-es": "^0.5.0",
		"date-picker-svelte": "^2.2.5",
		"eslint": "^8.28.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-storybook": "^0.6.11",
		"eslint-plugin-svelte3": "^4.0.0",
		"flatpickr": "^4.6.13",
		"husky": "^8.0.0",
		"jsonwebtoken": "^9.0.0",
		"postcss": "^8.4.21",
		"prettier": "^2.8.3",
		"prettier-plugin-svelte": "^2.8.1",
		"prettier-plugin-tailwindcss": "^0.2.2",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"storybook": "^7.0.0-beta.53",
		"svelte": "^3.54.0",
		"svelte-check": "^3.0.1",
		"tailwindcss": "^3.2.4",
		"tslib": "^2.4.1",
		"typescript": "^4.9.3",
		"vite": "^4.0.0",
		"vitest": "^0.25.3"
	},
	"type": "module",
	"dependencies": {
		"@sveltejs/adapter-node": "^1.1.4",
		"svelte-device-detector": "^0.2.2",
		"svelte-flatpickr": "^3.3.1"
	}
}
