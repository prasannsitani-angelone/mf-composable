{
	"name": "mf-sales-web",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev-debug": "NODE_OPTIONS=--inspect ./node_modules/.bin/vite dev",
		"dev": "vite dev",
		"dev:uat": "cp .env.uat .env && vite dev",
		"dev:cug": "cp .env.cug .env && vite dev",
		"build": "vite build",
		"build:uat": "cp .env.uat .env && vite build",
		"build:cug": "cp .env.cug .env && vite build",
		"build:prod": "cp .env.prod .env && vite build",
		"preview": "vite preview",
		"test": "playwright test",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"test:unit": "vitest",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write .",
		"prepare": "husky install"
	},
	"devDependencies": {
		"@playwright/test": "^1.28.1",
		"@sveltejs/adapter-auto": "2.0.0",
		"@sveltejs/kit": "1.12.0",
		"@typescript-eslint/eslint-plugin": "^5.45.0",
		"@typescript-eslint/parser": "^5.45.0",
		"autoprefixer": "^10.4.13",
		"cookie-es": "^0.5.0",
		"date-picker-svelte": "^2.2.5",
		"eslint": "^8.28.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"flatpickr": "^4.6.13",
		"husky": "^8.0.0",
		"jsonwebtoken": "^9.0.0",
		"postcss": "^8.4.21",
		"prettier": "^2.8.3",
		"prettier-plugin-svelte": "^2.8.1",
		"prettier-plugin-tailwindcss": "^0.2.2",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"svelte": "3.57.0",
		"svelte-carousel": "1.0.25",
		"svelte-check": "^3.0.1",
		"tailwindcss": "3.2.7",
		"tslib": "^2.4.1",
		"typescript": "^4.9.3",
		"vite": "^4.0.0",
		"vitest": "^0.25.3"
	},
	"type": "module",
	"dependencies": {
		"@sveltejs/adapter-node": "1.2.3",
		"chart.js": "4.2.1",
		"chartjs-adapter-date-fns": "3.0.0",
		"crypto-js": "4.1.1",
		"daisyui": "2.51.4",
		"date-fns": "^2.29.3",
		"lodash.merge": "^4.6.2",
		"svelte-chartjs": "3.1.2",
		"svelte-device-detector": "^0.2.2",
		"svelte-flatpickr": "^3.3.1",
		"svelte-range-slider-pips": "^2.1.1",
		"svelte-video-player": "^1.2.5",
		"uuid": "^9.0.0"
	}
}
