{
	"name": "mf-ui-web",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev-debug": "NODE_OPTIONS=--inspect ./node_modules/.bin/vite dev",
		"dev": "vite dev",
		"dev:uat": "cp .env.uat .env && JWT_SYMMETRIC_KEY=aAJALJFLJLSLFJ@##@!12123a vite dev --host",
		"dev:cug": "cp .env.cug .env && vite dev --host",
		"dev:prod": "cp .env.prod .env && vite dev --host",
		"build": "vite build",
		"build:uat": "cp .env.uat .env && vite build",
		"build:cug": "cp .env.cug .env && vite build",
		"build:prod": "cp .env.prod .env && vite build",
		"preview": "JWT_SYMMETRIC_KEY=aAJALJFLJLSLFJ@##@!12123a vite preview --host",
		"test": "playwright test",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"test:unit": "vitest",
		"lint": "eslint --cache .",
		"format": "prettier --plugin-search-dir . --cache --write .",
		"prepare": "husky install"
	},
	"devDependencies": {
		"@playwright/test": "1.28.1",
		"@sveltejs/kit": "1.22.0",
		"@typescript-eslint/eslint-plugin": "5.61.0",
		"@typescript-eslint/parser": "5.61.0",
		"autoprefixer": "10.4.14",
		"cookie-es": "0.5.0",
		"eslint": "8.44.0",
		"eslint-config-prettier": "8.8.0",
		"eslint-plugin-svelte3": "4.0.0",
		"fluent-svelte": "^1.6.0",
		"husky": "8.0.3",
		"jsonwebtoken": "9.0.0",
		"postcss": "8.4.25",
		"prettier": "3.0.0",
		"prettier-plugin-svelte": "2.10.1",
		"prettier-plugin-tailwindcss": "0.3.0",
		"svelte": "4.0.5",
		"svelte-check": "3.4.5",
		"tailwindcss": "3.3.2",
		"tslib": "2.6.0",
		"typescript": "5.1.6",
		"vite": "4.4.2",
		"vitest": "0.33.0",
		"rollup-plugin-visualizer": "5.9.2"
	},
	"type": "module",
	"dependencies": {
		"@bulatdashiev/svelte-slider": "1.0.3",
		"@sveltejs/adapter-node": "1.3.1",
		"chart.js": "4.3.0",
		"chartjs-adapter-date-fns": "3.0.0",
		"daisyui": "2.51.4",
		"date-fns": "2.30.0",
		"fflate": "0.7.4",
		"lodash.merge": "4.6.2",
		"servertime": "3.1.1",
		"svelte-chartjs": "3.1.2",
		"svelte-device-detector": "0.2.2",
		"svelte-intersection-observer": "0.10.0",
		"svelte-video-player": "1.2.5",
		"sveltekit-device-detector": "1.0.2",
		"ua-parser-js": "^1.0.35",
		"uuid": "^9.0.0",
		"svelte-components": "0.93.0",
		"word-wrap": "^1.2.3"
	}
}
