	"scripts": {
		"dev-debug": "NODE_OPTIONS=--inspect ./node_modules/.bin/vite dev",
		"dev": "vite dev",
		"dev:uat": "cp .env.uat .env && JWT_SYMMETRIC_KEY=aAJALJFLJLSLFJ@##@!12123a vite dev --host",
